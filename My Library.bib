  author = {Bryan, Gwendolyn M. and Franks, Patrick W. and Song, Seungmoon and Voloshina, Alexandra S. and Reyes, Ricardo and O'Donovan, Meghan P. and Gregorczyk, Karen N. and Collins, Steven H.},
  year = {2021},
  month = oct,
  journal = {Journal of NeuroEngineering and Rehabilitation},
  volume = {18},
  number = {1},
  pages = {152},
  issn = {1743-0003},
  doi = {10.1186/s12984-021-00943-y},
  urldate = {2023-01-16},
  abstract = {Autonomous exoskeletons will need to be useful at a variety of walking speeds, but it is unclear how optimal hip{\textendash}knee{\textendash}ankle exoskeleton assistance should change with speed. Biological joint moments tend to increase with speed, and in some cases, optimized ankle exoskeleton torques follow a similar trend. Ideal hip{\textendash}knee{\textendash}ankle exoskeleton torque may also increase with speed. The purpose of this study was to characterize the relationship between walking speed, optimal hip{\textendash}knee{\textendash}ankle exoskeleton assistance, and the benefits to metabolic energy cost.},
  keywords = {Data-driven control,exoskeleton,Reinforcement learning},
  file = {C\:\\Users\\zjiefu\\Zotero\\storage\\F48JRC8Q\\Bryan et al. - 2021 - Optimized hip–knee–ankle exoskeleton assistance at.pdf;C\:\\Users\\zjiefu\\Zotero\\storage\\HT34MVI3\\s12984-021-00943-y.html}
}

