@article{jones30sChairStandTest1999,
  title = {A 30-s {{Chair-Stand Test}} as a {{Measure}} of {{Lower Body Strength}} in {{Community-Residing Older Adults}}},
  author = {Jones, C. Jessie and Rikli, Roberta E. and Beam, William C.},
