@article{schmitzModulationAchillesTendon2022,
  title = {Modulation of {{Achilles}} Tendon Force with Load Carriage and Exosuit Assistance},
  author = {Schmitz, Dylan G. and Nuckols, Richard W. and Lee, Sangjun and Akbas, Tunc and Swaminathan, Krithika and Walsh, Conor J. and Thelen, Darryl G.},
