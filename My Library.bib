  keywords = {Data-driven control,Prothesis,Reinforcement learning},
  file = {C:\Users\zjiefu\Zotero\storage\RBI6JXQ8\Wu et al. - 2022 - Reinforcement Learning Impedance Control of a Robo.pdf}
}

@article{wuRoboticKneeTracking2022,
