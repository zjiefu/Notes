@misc{zhangOptimalEnergyShaping2022,
  title = {Optimal {{Energy Shaping Control}} for a {{Backdrivable Hip Exoskeleton}}},
  author = {Zhang, Jiefu and Lin, Jianping and Peddinti, Vamsi and Gregg, Robert D.},
