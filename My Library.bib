@article{rosenbergQuantifyingChangesIndividualspecific2024,
  title = {Quantifying Changes in Individual-Specific Template-Based Representations of Center-of-Mass Dynamics during Walking with Ankle Exoskeletons Using {{Hybrid-SINDy}}},
  author = {Rosenberg, Michael C. and Proctor, Joshua L. and Steele, Katherine M.},
